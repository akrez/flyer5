{
    "minimum-stability": "stable",
    "require": {
        "php": ">=7",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-twig": "*",
        "kartik-v/yii2-widget-select2": "*",
        "yiisoft/yii2-jui": "*",
        "yiisoft/yii2-imagine": "*",
        "jeroendesloovere/vcard": "*",
        "yiisoft/yii2-bootstrap4": "*",
        "yiisoft/yii2-httpclient": "*",
        "php-amqplib/php-amqplib": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "codeception/base": "*",
        "codeception/verify": "*",
        "codeception/specify": "*"
    },
    "config": {
        "disable-tls": true,
        "secure-http": false,
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
